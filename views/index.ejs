<!DOCTYPE html>
<html lang="en">
  <head>
    <title>Smart Todo List</title>

    <link rel="stylesheet" href="/vendor/normalize-4.1.1.css" type="text/css" />
    <link rel="stylesheet" href="/vendor/border-box.css" type="text/css" />
    <link rel="stylesheet" href="/styles/layout.css" type="text/css" />
    <link rel="stylesheet" href="/styles/nav.css" type="text/css" />
    <link rel="stylesheet" href="/styles/lists.css" type="text/css" />
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.3.1/css/all.css" integrity="sha384-mzrmE5qonljUremFsqc01SB46JvROS7bZs3IO2EmfFsd15uHvIt+Y8vEf7N7fWAU" crossorigin="anonymous">

    <script type="text/javascript" src="/vendor/jquery-3.0.0.js"></script>
    <script type="text/javascript" src="/scripts/app.js"></script>
    <link rel="stylesheet" href="/jqueryui/jquery-ui.min.css">
    <script src="/jqueryui/external/jquery/jquery.js"></script>
    <script src="/jqueryui/jquery-ui.min.js"></script>

  </head>
  <body>
    <nav id="nav-bar">
      <div class="welcome">
        <h2 class="welcome">Welcome!</h2>
        <h4 class="name"></h2>
      </div>
      <div>
        <span class="header">SMART<br>Todo List</span>
      </div>
      <div class="login-new-list">
        <button id="new-list">
          New List
        </button>
        <button class="login-nav" id="login">Login</button>
      </div>
      <input class="new-list-input" type="text" placeholder="Add SMART List">
    <form class="login-dropdown">
      <div class="form-group">
        <input type="username" placeholder="username">
      </div>
      <div class="form-group">
        <input type="password" placeholder="Password">
      </div>
      <button class="register" type="submit">Register</button>
      <button class="sign-in" type="submit" >Sign in</button>
    </form>
    </nav>
    <section class="todos">
      <div class="lists">
        <div class="list-title">
          <span class="deleteButton"><i class="fas fa-trash"></i></span>
          <h2>Things TODO</h2>
          <span class="plus"><i class="fas fa-plus"></i></span>
        </div>
        <input class="new-todo-input" type="text" placeholder="Add TODO">
        <div class="list-dropdown">
          <h3>Watch</h3>
          <ul class="watch">
            <li><span class="delete"><i class="fas fa-times"></i></span>Venom</li>
            <li><span class="delete"><i class="fas fa-times"></i></span>Minons</li>
            <li><span class="delete"><i class="fas fa-times"></i></span>Avengers</li>
          </ul>
          <h3>Eat</h3>
          <ul class="eat">
            <li><span class="delete"><i class="fas fa-times"></i></span>Kayomono</li>
          </ul>
          <h3>Buy</h3>
          <ul class="buy">
            <li><span class="delete"><i class="fas fa-times"></i></span>New couch</li>
          </ul>
          <h3>Read</h3>
          <ul class="read">
            <li><span class="delete"><i class="fas fa-times"></i></span>Harry Potter</li>
          </ul>
          <h3>Play</h3>
          <ul class="play">
            <li><span class="delete"><i class="fas fa-times"></i></span>Just Cause 4</li>
          </ul>
          <h3>Other</h3>
          <ul class="other">
          </ul>
        </div>
      </div>
